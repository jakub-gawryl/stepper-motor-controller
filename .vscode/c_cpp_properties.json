{
  "configurations": [
    {
      "name": "AVR",
      "intelliSenseMode": "${default}",
      "compilerPath": "/usr/bin/avr-gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "includePath": [
        "/usr/lib/avr/lib/**"
      ],
      "compilerArgs": [
        "-g",
        "-Os",
        "-Wall",
        "-Wextra",
        "-fpermissive",
        "-fno-exceptions",
        "-fno-threadsafe-statics",
        "-pipe"
       ],
       "defines": [
        "F_CPU=8000000UL",
        "__AVR_ATmega328P__"
       ]
    }
  ],
  "version": 4
}