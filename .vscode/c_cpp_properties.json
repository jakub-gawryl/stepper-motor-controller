{
    "configurations": [
        {
            "name": "AVR",
            "intelliSenseMode": "${default}",
            "compilerPath": "/usr/bin/avr-gcc",
            "cStandard": "${default}",
            "cppStandard": "${default}",
            "includePath": [
                "/usr/lib/avr/lib/**"
            ],
            "compilerArgs": [
                "-g",
                "-Os",
                "-Wall",
                "-Wextra",
                "-fpermissive",
                "-fno-exceptions",
                "-fno-threadsafe-statics",
                "-pipe"
            ]
        }
    ],
    "version": 4
}